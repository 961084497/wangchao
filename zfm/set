a = [1,2,3,4,5]
>>> b = [4,5,6,7,8]
set(a) | set(b)
set([1, 2, 3, 4, 5, 6, 7, 8])
>>> list(set(a) | set(b))
[1, 2, 3, 4, 5, 6, 7, 8]
>>> set(a) & set(b)
set([4, 5])
>>> list(set(a) & set(b))
[4, 5]
>>> list(set(a) - set(b))
[1, 2, 3]
>>> list(set(b) - set(a))                                                                                                                                                                              [8, 6, 7]
>>>list(set(a) ^ set(b))
[1, 2, 3, 7, 8, 9]
